<body>
  <div class="antialiased start-below">
    <div id="app" class="flex min-h-screen bg-gray-50">
      <div class="fixed inset-0 z-10 lg:hidden" :class="{ hidden: !sidebarActive }">
        <div class="absolute inset-0 bg-gray-500 opacity-75"></div>
      </div>
      <aside :class="sidebarActive ? 'translate-x-0' : '-translate-x-full'" class="fixed top-0 z-50 flex flex-col flex-shrink-0 w-64 h-screen transition duration-150 ease-in-out transform bg-gray-100 border-r border-gray-200 lg:translate-x-0 lg:sticky">
        <div class="relative flex items-center h-16 px-5">
          <%= embed_svg "logos/indigo-dark-logo", class: "h-8 w-auto" %>
          <div class="absolute top-0 right-0 p-2 -mr-14">
            <button @click="sidebarActive = false" :class="{ hidden: !sidebarActive }" class="flex items-center p-2 bg-gray-700 rounded-full lg:hidden focus:outline-none focus:bg-gray-500">
              <%= embed_svg "stroke/x", class: "w-6 h-6 text-white" %>
            </button>
          </div>
        </div>
        <div class="hidden px-3 mt-2 lg:items-center lg:flex">
          <button class="flex items-center w-full p-2 transition duration-150 ease-in-out rounded-md focus:outline-none focus:bg-gray-200 hover:bg-gray-200">
            <span class="flex-shrink-0 block">
              <img class="w-10 h-10 rounded-full" src="https://images.unsplash.com/photo-1495490140452-5a226aef25d4?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80" alt="User profile">
            </span>
            <span class="flex items-center justify-between flex-1">
              <p class="flex flex-col pl-3">
                <span class="text-sm font-medium leading-5 text-gray-700">Jessy Schwarz</span>
                <span class="text-sm font-medium leading-5 text-gray-500">@jessyschwarz</span>
              </p>
              <%= embed_svg "solid/selector", class: "w-5 h-5 text-gray-600" %>
            </span>
          </button>
        </div>
        <div class="relative hidden px-3 py-6 lg:items-center lg:flex">
          <input type="text" placeholder="Search" class="block w-full pl-10 shadow-sm form-input sm:text-sm">
          <div class="absolute left-0 pl-5 pointer-events-none">
            <%= embed_svg "stroke/search", class: "w-5 h-5 text-gray-500" %>
          </div>
        </div>
        <div class="mt-4 space-y-8 lg:mt-0">
          <nav class="px-3 space-y-1 text-sm font-medium">
            <a href="#" class="flex items-center p-2 transition duration-150 ease-in-out bg-gray-200 rounded-md group">
              <%= embed_svg "stroke/home", class: "-ml-0.5 w-6 h-6 text-gray-700 transition duration-150 ease-in-out group-hover:text-gray-700" %>
              <span class="pl-3 leading-5 tracking-wide text-gray-900 transition duration-150 ease-in-out group-hover:text-gray-900">
                Dashboard
              </span>
            </a>
            <a href="#" class="flex items-center p-2 rounded-md group">
              <%= embed_svg "stroke/view-list", class: "-ml-0.5 w-6 h-6 text-gray-500 transition duration-150 ease-in-out group-hover:text-gray-700" %>
              <span class="pl-3 leading-5 tracking-wide text-gray-700 transition duration-150 ease-in-out group-hover:text-gray-900">
                My tasks
              </span>
            </a>
            <a href="#" class="flex items-center p-2 rounded-md group">
              <%= embed_svg "stroke/clock", class: "-ml-0.5 w-6 h-6 text-gray-500 transition duration-150 ease-in-out group-hover:text-gray-700" %>
              <span class="pl-3 leading-5 tracking-wide text-gray-700 transition duration-150 ease-in-out group-hover:text-gray-900">
                Recent
              </span>
            </a>
          </nav>
          <div class="px-5">
            <h3 class="text-xs font-semibold tracking-wide text-gray-500 uppercase">Teams</h3>
            <nav class="mt-3 space-y-4 text-sm font-medium">
              <p class="flex items-center">
                <span class="flex-shrink-0 w-2 h-2 bg-indigo-500 rounded-full"></span>
                <a href="#" class="ml-3 text-gray-700 transition duration-150 ease-in-out hover:text-gray-900">
                  Engineering
                </a>
              </p>
              <p class="flex items-center">
                <span class="flex-shrink-0 w-2 h-2 bg-pink-500 rounded-full"></span>
                <a href="#" class="ml-3 text-gray-700 transition duration-150 ease-in-out hover:text-gray-900">
                  Human Resources
                </a>
              </p>
              <p class="flex items-center">
                <span class="flex-shrink-0 w-2 h-2 bg-teal-500 rounded-full"></span>
                <a href="#" class="ml-3 text-gray-700 transition duration-150 ease-in-out hover:text-gray-900">
                  Customer Success
                </a>
              </p>
            </nav>
          </div>
        </div>
      </aside>
      <%# End header %>
      <div class="flex flex-col flex-1 overflow-y-auto">
        <header class="bg-white border-b border-gray-200 lg:px-8">
          <div class="flex items-center justify-between h-16">
            <button @click="sidebarActive = true" class="h-16 px-4 text-gray-500 transition duration-150 ease-in-out border-r border-gray-200 lg:hidden hover:text-gray-600 focus:outline-none focus:text-gray-600 focus:bg-gray-200">
              <%= embed_svg "stroke/menu-alt-1", class: "w-6 h-6" %>
            </button>
            <div class="relative flex items-center flex-1 h-16 lg:hidden">
              <input type="text" placeholder="Search" class="block w-full py-3 pl-10 pr-3 text-base text-gray-700 border-0 appearance-none sm:text-sm sm:leading-5 lg:pl-8 focus:outline-none">
              <div class="absolute pl-3 pointer-events-none lg:pl-0">
                <%= embed_svg "stroke/search", class: "w-5 h-5 text-gray-500" %>
              </div>
            </div>
            <div class="flex items-center px-4 space-x-3 lg:hidden">
              <div class="relative">
                <button @click="showProfile = !showProfile" class="flex items-center text-sm transition duration-150 ease-in-out border-2 border-transparent rounded-full focus:outline-none focus:border-gray-400">
                  <img class="w-8 h-8 rounded-full" src="https://images.unsplash.com/photo-1488426862026-3ee34a7d66df?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=144&h=144&q=80" alt="User profile" />
                </button>
                <div :class="{ hidden: !showProfile }" class="absolute right-0 w-48 mt-2 origin-top-right rounded-md shadow-lg">
                  <div class="py-1 bg-white rounded-md shadow-xs">
                    <a href="#" class="block px-4 py-2 text-sm leading-5 text-gray-700 transition duration-150 ease-in-out hover:bg-gray-100 focus:outline-none focus:bg-gray-100" role="menuitem">
                      Your Profile
                    </a>
                    <a href="#" class="block px-4 py-2 text-sm leading-5 text-gray-700 transition duration-150 ease-in-out hover:bg-gray-100 focus:outline-none focus:bg-gray-100" role="menuitem">
                      Settings
                    </a>
                    <a href="#" class="block px-4 py-2 text-sm leading-5 text-gray-700 transition duration-150 ease-in-out hover:bg-gray-100 focus:outline-none focus:bg-gray-100" role="menuitem">
                      Sign out
                    </a>
                  </div>
                </div>
              </div>
            </div>
            <div class="hidden w-full lg:flex lg:items-center lg:justify-between">
              <div>
                <h1 class="text-lg font-semibold tracking-wide text-gray-900">Home</h1>
              </div>
              <div class="space-x-3">
                <span class="rounded-md shadow-sm">
                  <button class="inline-flex items-center px-3 py-2 text-sm font-medium leading-5 text-gray-700 whitespace-no-wrap transition duration-150 ease-in-out bg-white border border-gray-300 rounded-md hover:text-gray-500 focus:outline-none focus:shadow-outline-blue focus:border-blue-300 active:text-gray-800 active:bg-gray-50">
                    Share
                  </button>
                </span>
                <span class="rounded-md shadow-sm">
                  <button class="inline-flex items-center px-3 py-2 text-sm font-medium leading-5 text-white whitespace-no-wrap transition duration-150 ease-in-out bg-indigo-600 border border-transparent rounded-md hover:bg-indigo-500 focus:outline-none focus:border-indigo-700 focus:shadow-outline-indigo active:bg-indigo-700">
                    Create
                  </button>
                </span>
              </div>
            </div>
          </div>
        </header>
        <%# End header %>
        <main>
          <div class="p-4 bg-white border-b border-gray-200 lg:hidden sm:px-6 lg:px-8">
            <h1 class="font-semibold tracking-wide text-gray-900">Home</h1>
            <div class="mt-3 space-x-2">
              <span class="rounded-md shadow-sm">
                <button class="inline-flex items-center px-3 py-2 text-sm font-medium leading-5 text-white whitespace-no-wrap transition duration-150 ease-in-out bg-indigo-600 border border-transparent rounded-md hover:bg-indigo-500 focus:outline-none focus:border-indigo-700 focus:shadow-outline-indigo active:bg-indigo-700">
                  Create
                </button>
              </span>
              <span class="rounded-md shadow-sm">
                <button class="inline-flex items-center px-3 py-2 text-sm font-medium leading-5 text-gray-700 whitespace-no-wrap transition duration-150 ease-in-out bg-white border border-gray-300 rounded-md hover:text-gray-500 focus:outline-none focus:shadow-outline-blue focus:border-blue-300 active:text-gray-800 active:bg-gray-50">
                  Share
                </button>
              </span>
            </div>
          </div>
          <div class="px-4 mt-6 sm:px-6 lg:px-8">
            <div>
              <h3 class="text-xs font-semibold leading-6 tracking-wide text-gray-500 uppercase">
                Pinned projects
              </h3>
              <div class="grid gap-5 mt-3 sm:grid-cols-2 xl:grid-cols-4">
                <div class="flex items-center overflow-hidden bg-white rounded-md shadow-sm sm:col-span-1">
                  <div class="flex items-center justify-center flex-shrink-0 w-16 h-16 bg-green-400">
                    <span class="text-base font-medium leading-6 tracking-wide text-white">
                      RC
                    </span>
                  </div>
                  <div class="flex items-center justify-between flex-1 h-16 min-w-0 px-4 border border-l-0 border-gray-200">
                    <div class="text-base truncate">
                      <p class="text-sm font-medium leading-6 text-gray-900 truncate">
                        React components
                      </p>
                      <p class="text-sm leading-6 text-gray-500">
                        8 members
                      </p>
                    </div>
                    <div class="-mr-2">
                      <button class="p-2 text-gray-500 transition duration-150 ease-in-out rounded-full hover:bg-gray-200 hover:text-gray-700 focus:outline-none focus:bg-gray-200 focus:text-gray-700">
                        <%= embed_svg "stroke/dots-vertical", class: "w-6 h-6" %>
                      </button>
                    </div>
                  </div>
                </div>
                <div class="flex items-center overflow-hidden bg-white rounded-md shadow-sm sm:col-span-1">
                  <div class="flex items-center justify-center flex-shrink-0 w-16 h-16 bg-pink-500">
                    <span class="text-base font-medium leading-6 tracking-wide text-white">
                      GQ
                    </span>
                  </div>
                  <div class="flex items-center justify-between flex-1 h-16 min-w-0 px-4 border border-l-0 border-gray-200">
                    <div class="text-base truncate">
                      <p class="text-sm font-medium leading-6 text-gray-900 truncate">
                        GraphQL
                      </p>
                      <p class="text-sm leading-6 text-gray-500">
                        7 members
                      </p>
                    </div>
                    <div class="-mr-2">
                      <button class="p-2 text-gray-500 transition duration-150 ease-in-out rounded-full hover:bg-gray-200 hover:text-gray-700 focus:outline-none focus:bg-gray-200 focus:text-gray-700">
                        <%= embed_svg "stroke/dots-vertical", class: "w-6 h-6" %>
                      </button>
                    </div>
                  </div>
                </div>
                <div class="flex items-center overflow-hidden bg-white rounded-md shadow-sm sm:col-span-1">
                  <div class="flex items-center justify-center flex-shrink-0 w-16 h-16 bg-purple-400">
                    <span class="text-base font-medium leading-6 tracking-wide text-white">
                      CD
                    </span>
                  </div>
                  <div class="flex items-center justify-between flex-1 h-16 min-w-0 px-4 border border-l-0 border-gray-200">
                    <div class="text-base truncate">
                      <p class="text-sm font-medium leading-6 text-gray-900 truncate">
                        Component Design
                      </p>
                      <p class="text-sm leading-6 text-gray-500">
                        11 members
                      </p>
                    </div>
                    <div class="-mr-2">
                      <button class="p-2 text-gray-500 transition duration-150 ease-in-out rounded-full hover:bg-gray-200 hover:text-gray-700 focus:outline-none focus:bg-gray-200 focus:text-gray-700">
                        <%= embed_svg "stroke/dots-vertical", class: "w-6 h-6" %>
                      </button>
                    </div>
                  </div>
                </div>
                <div class="flex items-center overflow-hidden bg-white rounded-md shadow-sm sm:col-span-1">
                  <div class="flex items-center justify-center flex-shrink-0 w-16 h-16 bg-orange-400">
                    <span class="text-base font-medium leading-6 tracking-wide text-white">
                      T
                    </span>
                  </div>
                  <div class="flex items-center justify-between flex-1 h-16 min-w-0 px-4 border border-l-0 border-gray-200">
                    <div class="text-base truncate">
                      <p class="text-sm font-medium leading-6 text-gray-900 truncate">
                        Templates
                      </p>
                      <p class="text-sm leading-6 text-gray-500">
                        6 members
                      </p>
                    </div>
                    <div class="-mr-2">
                      <button class="p-2 text-gray-500 transition duration-150 ease-in-out rounded-full hover:bg-gray-200 hover:text-gray-700 focus:outline-none focus:bg-gray-200 focus:text-gray-700">
                        <%= embed_svg "stroke/dots-vertical", class: "w-6 h-6" %>
                      </button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <%# End grid list %>
          <div class="flex flex-col px-4 mt-6 sm:px-6 lg:px-8">
            <h3 class="text-xs font-semibold leading-6 tracking-wide text-gray-500 uppercase">
              Recent activity
            </h3>
            <div class="mt-3">
              <div class="overflow-hidden rounded-lg shadow">
                <div class="inline-block w-full overflow-x-auto align-middle border-b border-gray-200">
                  <table class="min-w-full divide-y divide-gray-200">
                    <thead>
                      <tr>
                        <th class="px-6 py-3 text-xs font-medium leading-4 tracking-wider text-left text-gray-500 uppercase whitespace-no-wrap bg-gray-50">
                          Project
                        </th>
                        <th class="px-6 py-3 text-xs font-medium leading-4 tracking-wider text-left text-gray-500 uppercase whitespace-no-wrap bg-gray-50">
                          Members
                        </th>
                        <th class="px-6 py-3 text-xs font-medium leading-4 tracking-wider text-right text-gray-500 uppercase whitespace-no-wrap bg-gray-50">
                          Last updated
                        </th>
                        <th class="px-6 py-3 whitespace-no-wrap bg-gray-50">
                        </th>
                      </tr>
                    </thead>
                    <tbody class="bg-white divide-y divide-gray-200">
                      <tr v-for="(transaction, i) in transactions" :key="i">
                        <td class="px-6 py-4 whitespace-no-wrap">
                          <div class="flex items-center">
                            <span :class="transaction.pillClass" class="flex-shrink-0 w-2 h-2 rounded-full"></span>
                            <span class="pl-3 text-sm leading-5 text-gray-500">
                              <span class="font-medium text-gray-900">
                                {{ transaction.name }}
                              </span>
                              in {{ transaction.category }}
                            </span>
                          </div>
                        </td>
                        <td class="px-6 py-4 text-left whitespace-no-wrap">
                          <span class="text-sm font-medium leading-5 text-gray-900">
                            <div class="flex">
                              <img class="inline-block w-6 h-6 text-white rounded-full shadow-solid" src="https://images.unsplash.com/photo-1491528323818-fdd1faba62cc?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80" alt="" />
                              <img class="inline-block w-6 h-6 -ml-1 text-white rounded-full shadow-solid" src="https://images.unsplash.com/photo-1550525811-e5869dd03032?ixlib=rb-1.2.1&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80" alt="" />
                              <img class="inline-block w-6 h-6 -ml-1 text-white rounded-full shadow-solid" src="https://images.unsplash.com/photo-1500648767791-00dcc994a43e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2.25&w=256&h=256&q=80" alt="" />
                              <img class="inline-block w-6 h-6 -ml-1 text-white rounded-full shadow-solid" src="https://images.unsplash.com/photo-1472099645785-5658abf4ff4e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80" alt="" />
                            </div>
                          </span>
                        </td>
                        <td class="px-6 py-4 text-sm leading-5 text-right text-gray-500 whitespace-no-wrap">
                          {{ transaction.date }}
                        </td>
                        <td class="flex items-center justify-end px-6 py-3">
                          <button class="p-1 text-gray-500 transition duration-150 ease-in-out border-2 border-transparent rounded-full focus:outline-none focus:text-gray-600 focus:bg-gray-200 hover:text-gray-600 hover:bg-gray-200">
                            <%= embed_svg "solid/vertical-dots", class: "w-5 h-5" %>
                          </button>
                        </td>
                      </tr>
                      <!-- More rows... -->
                    </tbody>
                  </table>
                </div>
              </div>
              <%# End table %>
              <div class="flex items-center justify-between py-3 mt-3">
                <div class="flex justify-between flex-1 sm:hidden">
                  <a href="#" class="relative inline-flex items-center px-4 py-2 text-sm font-medium leading-5 text-gray-700 transition duration-150 ease-in-out bg-white border border-gray-300 rounded-md hover:text-gray-500 focus:outline-none focus:shadow-outline-blue focus:border-blue-300 active:bg-gray-100 active:text-gray-700">
                    Previous
                  </a>
                  <a href="#" class="relative inline-flex items-center px-4 py-2 ml-3 text-sm font-medium leading-5 text-gray-700 transition duration-150 ease-in-out bg-white border border-gray-300 rounded-md hover:text-gray-500 focus:outline-none focus:shadow-outline-blue focus:border-blue-300 active:bg-gray-100 active:text-gray-700">
                    Next
                  </a>
                </div>
                <div class="hidden sm:flex-1 sm:flex sm:items-center sm:justify-between">
                  <div>
                    <p class="text-sm leading-5 text-gray-700">
                      Showing
                      <span class="font-medium">1</span>
                      to
                      <span class="font-medium">10</span>
                      of
                      <span class="font-medium">97</span>
                      results
                    </p>
                  </div>
                  <div>
                    <nav class="relative z-0 inline-flex shadow-sm">
                      <a href="#" class="relative inline-flex items-center px-2 py-2 text-sm font-medium leading-5 text-gray-500 transition duration-150 ease-in-out bg-white border border-gray-300 rounded-l-md hover:text-gray-400 focus:z-10 focus:outline-none focus:border-blue-300 focus:shadow-outline-blue active:bg-gray-100 active:text-gray-500" aria-label="Previous">
                        <%= embed_svg "solid/chevron-left", class: "w-5 h-5" %>
                      </a>
                      <a href="#" class="relative inline-flex items-center px-4 py-2 -ml-px text-sm font-medium leading-5 text-gray-700 transition duration-150 ease-in-out bg-white border border-gray-300 hover:text-gray-500 focus:z-10 focus:outline-none focus:border-blue-300 focus:shadow-outline-blue active:bg-gray-100 active:text-gray-700">
                        1
                      </a>
                      <a href="#" class="relative inline-flex items-center px-4 py-2 -ml-px text-sm font-medium leading-5 text-gray-700 transition duration-150 ease-in-out bg-white border border-gray-300 hover:text-gray-500 focus:z-10 focus:outline-none focus:border-blue-300 focus:shadow-outline-blue active:bg-gray-100 active:text-gray-700">
                        2
                      </a>
                      <a href="#" class="relative items-center hidden px-4 py-2 -ml-px text-sm font-medium leading-5 text-gray-700 transition duration-150 ease-in-out bg-white border border-gray-300 md:inline-flex hover:text-gray-500 focus:z-10 focus:outline-none focus:border-blue-300 focus:shadow-outline-blue active:bg-gray-100 active:text-gray-700">
                        3
                      </a>
                      <span class="relative inline-flex items-center px-4 py-2 -ml-px text-sm font-medium leading-5 text-gray-700 bg-white border border-gray-300">
                        ...
                      </span>
                      <a href="#" class="relative items-center hidden px-4 py-2 -ml-px text-sm font-medium leading-5 text-gray-700 transition duration-150 ease-in-out bg-white border border-gray-300 md:inline-flex hover:text-gray-500 focus:z-10 focus:outline-none focus:border-blue-300 focus:shadow-outline-blue active:bg-gray-100 active:text-gray-700">
                        8
                      </a>
                      <a href="#" class="relative inline-flex items-center px-4 py-2 -ml-px text-sm font-medium leading-5 text-gray-700 transition duration-150 ease-in-out bg-white border border-gray-300 hover:text-gray-500 focus:z-10 focus:outline-none focus:border-blue-300 focus:shadow-outline-blue active:bg-gray-100 active:text-gray-700">
                        9
                      </a>
                      <a href="#" class="relative inline-flex items-center px-4 py-2 -ml-px text-sm font-medium leading-5 text-gray-700 transition duration-150 ease-in-out bg-white border border-gray-300 hover:text-gray-500 focus:z-10 focus:outline-none focus:border-blue-300 focus:shadow-outline-blue active:bg-gray-100 active:text-gray-700">
                        10
                      </a>
                      <a href="#" class="relative inline-flex items-center px-2 py-2 -ml-px text-sm font-medium leading-5 text-gray-500 transition duration-150 ease-in-out bg-white border border-gray-300 rounded-r-md hover:text-gray-400 focus:z-10 focus:outline-none focus:border-blue-300 focus:shadow-outline-blue active:bg-gray-100 active:text-gray-500" aria-label="Next">
                        <%= embed_svg "solid/chevron-right", class: "w-5 h-5" %>
                      </a>
                    </nav>
                  </div>
                </div>
              </div>
              <%# End pagination %>
            </div>
          </div>
        </main>
      </div>
    </div>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/vue@2.6.11"></script>
  <script>
    new Vue({
      el: '#app',
      data: {
        sidebarActive: false,
        showProfile: false,
        transactions: [
          {
            name: 'GraphQL API',
            category: 'Engineering',
            date: 'July 11, 2020',
            pillClass: 'bg-indigo-500'
          },
          {
            name: 'New Benefits Plan',
            category: 'Human Resources',
            date: 'July 5, 2020',
            pillClass: 'bg-pink-500'
          },
          {
            name: 'Onboarding Emails',
            category: 'Customer Success',
            date: 'July 4, 2020',
            pillClass: 'bg-teal-500'
          },
          {
            name: 'Employee Recognition Program',
            category: 'Human Resources',
            date: 'July 1, 2020',
            pillClass: 'bg-pink-500'
          },
          {
            name: 'Ruby on Rails',
            category: 'Engineering',
            date: 'June 11, 2020',
            pillClass: 'bg-indigo-500'
          },
          {
            name: 'Implement NPS',
            category: 'Customer Success',
            date: 'June 3, 2020',
            pillClass: 'bg-teal-500'
          },
        ],
      }
    })
  </script>
</body>
